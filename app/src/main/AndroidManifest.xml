<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calistheicslogger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.ExerciseListActivity">
    </activity>
        <activity android:name=".Activities.NewExerciseActivity"  android:noHistory="true">
        </activity>
        <activity android:name=".Activities.TrackActivity"></activity>
        <activity android:name=".Activities.HistoryActivity"></activity>
        <activity android:name=".Activities.PersonalRecordsActivity"></activity>
        <activity android:name=".Activities.LockerActivity"></activity>
        <activity android:name=".Activities.ChartActivity"></activity>
        <activity android:name=".Activities.CalendarActivity"></activity>
    </application>

</manifest>